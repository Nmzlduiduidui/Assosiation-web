    "quill": "^1.3.7",
    "quill-image-drop-module": "^1.0.3",
    "vue": "^2.6.11",
    "vue-image-crop-upload": "^2.5.0",
    "vue-quill-editor": "^3.0.6",
    "vue-quill-editor-upload": "^1.1.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
